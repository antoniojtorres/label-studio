{
  "name": "Label Studio",
  "description": "Multi-type data labeling, annotation and exploration tool",
  "keywords": [
    "data annotation",
    "data labeling"
  ],
  "website": "https://labelstud.io",
  "repository": "https://github.com/HumanSignal/label-studio",
  "logo": "https://raw.githubusercontent.com/HumanSignal/label-studio/refs/heads/develop/images/heartex_icon_opossum_orang.svg",
  "stack": "container",
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "16"
      }
    }
  ],
  "env": {
    "DISABLE_SIGNUP_WITHOUT_LINK": {
      "description": "Disable signup for users without invite link",
      "value": "0",
      "required": false
    },
    "USERNAME": {
      "description": "EMAIL ADDRESS for default user",
      "value": "",
      "required": true
    },
    "PASSWORD": {
      "description": "Password for default user",
      "value": "",
      "required": true
    },
    "STORAGE_TYPE": {
      "description": "Permanent Storage Type",
      "value": "S3",
      "required": true
    },
    "STORAGE_AWS_ACCESS_KEY_ID": {
      "description": "S3 Access Key",
      "value": "",
      "required": true
    },
    "STORAGE_AWS_SECRET_ACCESS_KEY": {
      "description": "S3 Secret Key",
      "value": "",
      "required": true
    },
    "STORAGE_AWS_BUCKET_NAME": {
      "description": "S3 Bucket Name",
      "value": "",
      "required": true
    },
    "STORAGE_AWS_REGION_NAME": {
      "description": "AWS Region",
      "value": "us-east-1",
      "required": true
    },
    "STORAGE_AWS_FOLDER": {
      "description": "Path inside of bucket",
      "value": "",
      "required": false
    }
  }
}
